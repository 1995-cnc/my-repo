package pluralsight.DemoCode;

public class Helllo {

	public static void main(String[] args) {
		
		int val1 = 40;
		int val2 = 40;
		
		if(val1 > val2)
			System.out.println("Value 1 is bigger");
		else if(val1 < val2)
			System.out.println("value 2 is bigger");
		else
			System.out.println("value 1 and value 2 are equal");
		

	}

}
